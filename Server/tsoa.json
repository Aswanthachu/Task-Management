{
    "entryFile": "index.ts",
    "controllerPathGlobs": [
        "./src/controller/branchController.ts"
    ],
    "spec": {
        "outputDirectory": "./",
        "entryFile": "index.ts",
        "noImplicitAdditionalProperties": "ignore",
        "securityDefinitions": {
            "apiKey": {
                "type": "apiKey",
                "name": "X-API-Key",
                "in": "header"
            },
            "clientId": {
                "type": "apiKey",
                "name": "Client-Id",
                "in": "header"
            },
            "bearerAuth": {
                "type": "apiKey",
                "name": "Authorization",
                "in": "header"
            }
        }
    },
    "routes": {
        "routesDir": "routes",
        "middleware": "express"
    }
}
  